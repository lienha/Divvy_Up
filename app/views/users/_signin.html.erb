
<div class="row">
  <%= form_for(:session, url: sessions_path) do |t| %>
		<div id="error_explanation">
	   <%= flash[:notice] %>
		</div>
  <p>
    <%= t.label :email %> 
    <%= t.email_field :email %>
    <%= t.label :enter_password %> 
    <%= t.password_field :password %>
    <%= t.submit value="Sign in" %>
  </p>
	<% end %>

</div>